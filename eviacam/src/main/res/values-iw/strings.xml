   <string name="wizard_keyboard_remarks">רוב המקלדות לא עובדות עם עכבראש. כדי להפעיל את המקלדת התואמת המסופקת:</string>
   <string name="wizard_keyboard_not_configured">המקלדת אינה מוגדרת</string>
   <string name="wizard_keyboard_not_configured_confirm">האם להמשיך מבלי להגדיר את המקלדת?</string>
   <string name="wizard_pointer_speed">מהירות הסמן</string>
   <string name="wizard_pointer_speed_summary">עליך להניע את ראשך כדי להזיז את הסמן. כעת עליך לנסות להגיע לכל חלק במסך. ניתן להתאים את המהירויות אם יש צורך בכך.</string>
   <string name="wizard_configuration_settings">הגדרות תצורה</string>
   <string name="wizard_configuration_settings_summary">כדי להתאים את מהירות הסמן ואת ההגדרות הנוספות, עליך לגשת אל\nהגדרות &gt; נגישות&gt; EVA Facial Mouse &gt; הגדרות</string>
   <string name="wizard_settings">הגדרות</string>
   <string name="wizard_known_limitations">מגבלות ידועות</string>
   <string name="wizard_limitations_summary">עקב מגבלות במערכת Android, חלק מהתכונות אינן זמינות עדיין.
        \n- לא ניתן להשתמש במצלמה
        \n- לא ניתן לקבל שיחות בחלק מההתקנים
        \n- רוב המשחקים לא עובדים
        \n- יישומים כגון מפות, Earth, גלריה ודפדפנים עובדים באופן מוגבל
        \n- כפתורי הגלילה לא תמיד עובדים (למשל בדפדפנים). כדי לגלול במקרים כאלה, כדאי לנסות להשתמש במקלדת הניווט (מידע להלן)</string>
   <string name="wizard_skip_wizard">לדלג על האשף</string>
   <string name="wizard_close_wizard_question">לסגור את האשף?</string>
   <string name="wizard_eva_not_running">עכבראש אינו פעיל</string>
   <string name="wizard_eva_not_running_summary">כדי להפעיל את האשף ראשית עליך להפעיל את השירות EVA Facial Mouse (עכבראש)</string>
   <string name="close">סגירה</string>
   <string name="wizard_trying_to_detect_face">מתבצע ניסיון לזיהוי הפנים…</string>
   <string name="wizard_face_detected">הפנים זוהו</string>
   <string name="wizard_pre_click_summary">כדי לבצע פעולת לחיצה, עליך לעצור את סמן העכבר במקום המבוקש. לאחר מספר רגעים הלחיצה תופעל אוטומטית. אם הפריט שלהלן מסוגל לבצע יותר מפעולה אחת יופיע תפריט הקשר.</string>
   <string name="wizard_click_enabled_next">במסך הבא, הסמן עם תכונת הלחיצה יופעל</string>
   <string name="wizard_to_test_click">כדי לבחון לחיצה, ניתן להשתמש בכפתור שלהלן. לתשומת לבך: אם הלחיצה תבוצע מחוץ לכפתור יוצעו לך שתי אפשרויות גלילה.</string>
   <string name="wizard_to_continue_make_click">לפני שנמשיך בתהליך, עליך ללחוץ לחיצה אחת קצרה ואחת ארוכה על הכפתור</string>
   <string name="wizard_click_scroll_button_to_continue">יש ללחוץ על כפתור הגלילה שנמצא מימין למטה כדי להמשיך</string>
   <string name="wizard_notification_icon">סמל התרעה</string>
   <string name="wizard_notification_icon_summary">כדי להשהות ולהמשיך את הפעילות של עכבראש (לדוגמה בעת מתן תמיכה), ניתן להשתמש בסמל ההתרעה שלהלן.</string>
   <string name="wizard_we_finished">סיימנו!</string>
   <string name="wizard_run_wizard_again">כדי להפעיל את האשף שוב, עליך לגשת אל \nהגדרות &gt; נגישות &gt; EVA Facial Mouse &gt; הגדרות</string>
   <string name="wizard_full_enable_after">כל התכונות של עכבראש יופעלו לאחר האשף הזה</string>
